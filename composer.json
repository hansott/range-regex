{
    "name": "hansott/range-regex",
    "type": "library",
    "description": "Returns a regex-compatible range from two numbers, min and max.",
    "keywords": [
        "hansott",
        "numeric",
        "regex",
        "range",
        "validation",
        "brace",
        "expansion",
        "glob",
        "match",
        "ranges",
        "number"
    ],
    "homepage": "https://github.com/hansott/range-regex",
    "license": "MIT",
    "authors": [
        {
            "name": "Hans Ott",
            "email": "hansott@hotmail.be",
            "homepage": "https://hansott.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "HansOtt\\RangeRegex\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HansOtt\\RangeRegex\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
